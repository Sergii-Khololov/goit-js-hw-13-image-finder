{{#each this}}
  <div class='photo-card'>
    <img src='{{webformatURL}}' alt='{{tags}}' width='370px' height="250px" />

    <div class='stats'>
      <p class='stats-item'>
        <i class='material-icons'>
          <span class='icons'>
            thumb_up_alt
          </span></i>
        {{likes}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>
          <span class='icons'>
            visibility
          </span></i>
        {{views}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>
          <span class='icons'>
            comment
          </span></i>
        {{comments}}
      </p>
      <p class='stats-item'>
        <i class='material-icons'>
          <span class='icons'>
            cloud_download
          </span></i>
        {{downloads}}
      </p>
    </div>
  </div>
{{/each}}